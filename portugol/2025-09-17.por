// Estrutura caso

programa
{
	funcao inicio()
	{
		caracter operador

		escreva("Entre com operador: ")
		leia(operador)
		escolha(operador)
		{
			caso'+':
			escreva("Soma = ",6+4)
			pare

			caso'-':
			escreva("Subtração = ",6-4)
			pare

			caso contrario:
			escreva("Não é soma nem subtração.")
		}	
	}
}



